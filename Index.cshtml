@section scripts {
    <script type="text/javascript">
        $(function () {
            $(document).on('mouseenter', '.topography-image', function (e) {
                var target = $(this).attr('data-target');
                // console.log(target);
                //console.log('enter');
                if (typeof target != 'undefined') {
                    $('#description div').addClass('hidden-content');
                    $(target).removeClass('hidden-content');
                }
                //handle backdrop
                $(this).find('.back-drop').addClass('in');

            });
            $(document).on('mouseleave', '.topography-image', function (e) {
                var target = $(this).attr('data-target');
                //console.log(target);
                //console.log('leave');

                //handle backdrop
                $(this).find('.back-drop').removeClass('in');
                if (typeof target != 'undefined') {
                    $('#description div').addClass('hidden-content');
                    //
                    if ($('#general').hasClass('hidden-content'))
                        $('#general').removeClass('hidden-content');
                }
            });
            $(document).on('click', '#fqaToggle a', function (e) {
                e.preventDefault();
                $(this).addClass('hidden-content');
                $(this).siblings().removeClass('hidden-content');
                if ($(this).attr('data-toggle') == 'expand')
                    $('#fqa .panel-collapse').collapse('show');
                else
                    $('#fqa .panel-collapse').collapse('hide');
            });
        });
    </script>
}
